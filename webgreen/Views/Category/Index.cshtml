@model List<cats>
<!DOCTYPE html>
<html lang="en">
<head>
   
</head>
<body>
    <div class="container mt-5">

        <!-- Table with Bootstrap styling -->
        <table class="table table-hover">
            <thead class="thead-light">
                
                    <tr>
                        <th>Name</th>
                        <th>DisplayOrder</th>
                    </tr>
                
            </thead>
            <tbody>
                @foreach (var data in Model)
                {
                <tr>
                    <td>@data.name</td>
                    <td>@data.displayorder</td>
                </tr>
                }
            </tbody>
        </table>
    </div>

</body>
</html>