@model List<cats>

        <table class="table table-hover">
            <thead class="thead-light">
                
                    <tr>
                        <th>Name</th>
                        <th>DisplayOrder</th>
                    </tr>
                
            </thead>
            <tbody>
                @foreach (var data in Model)
                {
                <tr>
                    <td>@data.name</td>
                    <td>@data.displayorder</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
    <!-- Include Bootstrap JS and jQuery -->
   
</body>
</html>