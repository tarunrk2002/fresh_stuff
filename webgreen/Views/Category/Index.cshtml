@model List<cats>
<!DOCTYPE html>
<html lang="en">
<head>
    <div class="container">
        <div class="row pt-3">
            <div class="col-6">
                <h2>Category List</h2>
            </div>

            <div class="col-6" text-end>
               
                <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Privacy">
                    <div class="btn btn-primary">
                        click here
                        
                    </div>
                </a>
            </div>

        </div>
    </div>
</head>
<body>
    <div class="container mt-5">

        <!-- Table with Bootstrap styling -->
        <table class="table table-hover">
            <thead class="thead-light">
                
                    <tr>
                        <th>Name</th>
                        <th>DisplayOrder</th>
                    </tr>
                
            </thead>
            <tbody>
                @foreach (var data in Model)
                {
                <tr>
                    <td>@data.name</td>
                    <td>@data.displayorder</td>
                </tr>
                }
            </tbody>
        </table>
    </div>

</body>
</html>